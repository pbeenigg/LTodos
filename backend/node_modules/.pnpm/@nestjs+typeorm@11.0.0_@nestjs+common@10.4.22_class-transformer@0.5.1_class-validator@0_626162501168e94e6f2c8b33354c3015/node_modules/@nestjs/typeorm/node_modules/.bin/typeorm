#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/pbeenig/Work/远程工作/临行网络科技/backend/node_modules/.pnpm/typeorm@0.3.28_pg@8.17.1_ts-node@10.9.2_@types+node@20.19.29_typescript@5.9.3_/node_modules/typeorm/node_modules:/Users/pbeenig/Work/远程工作/临行网络科技/backend/node_modules/.pnpm/typeorm@0.3.28_pg@8.17.1_ts-node@10.9.2_@types+node@20.19.29_typescript@5.9.3_/node_modules:/Users/pbeenig/Work/远程工作/临行网络科技/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/pbeenig/Work/远程工作/临行网络科技/backend/node_modules/.pnpm/typeorm@0.3.28_pg@8.17.1_ts-node@10.9.2_@types+node@20.19.29_typescript@5.9.3_/node_modules/typeorm/node_modules:/Users/pbeenig/Work/远程工作/临行网络科技/backend/node_modules/.pnpm/typeorm@0.3.28_pg@8.17.1_ts-node@10.9.2_@types+node@20.19.29_typescript@5.9.3_/node_modules:/Users/pbeenig/Work/远程工作/临行网络科技/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../typeorm@0.3.28_pg@8.17.1_ts-node@10.9.2_@types+node@20.19.29_typescript@5.9.3_/node_modules/typeorm/cli.js" "$@"
else
  exec node  "$basedir/../../../../../../typeorm@0.3.28_pg@8.17.1_ts-node@10.9.2_@types+node@20.19.29_typescript@5.9.3_/node_modules/typeorm/cli.js" "$@"
fi
