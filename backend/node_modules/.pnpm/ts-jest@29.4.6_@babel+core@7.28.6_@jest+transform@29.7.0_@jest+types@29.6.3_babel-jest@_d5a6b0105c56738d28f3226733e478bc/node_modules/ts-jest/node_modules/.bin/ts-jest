#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/pbeenig/Work/远程工作/临行网络科技/backend/node_modules/.pnpm/ts-jest@29.4.6_@babel+core@7.28.6_@jest+transform@29.7.0_@jest+types@29.6.3_babel-jest@_d5a6b0105c56738d28f3226733e478bc/node_modules/ts-jest/node_modules:/Users/pbeenig/Work/远程工作/临行网络科技/backend/node_modules/.pnpm/ts-jest@29.4.6_@babel+core@7.28.6_@jest+transform@29.7.0_@jest+types@29.6.3_babel-jest@_d5a6b0105c56738d28f3226733e478bc/node_modules:/Users/pbeenig/Work/远程工作/临行网络科技/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/pbeenig/Work/远程工作/临行网络科技/backend/node_modules/.pnpm/ts-jest@29.4.6_@babel+core@7.28.6_@jest+transform@29.7.0_@jest+types@29.6.3_babel-jest@_d5a6b0105c56738d28f3226733e478bc/node_modules/ts-jest/node_modules:/Users/pbeenig/Work/远程工作/临行网络科技/backend/node_modules/.pnpm/ts-jest@29.4.6_@babel+core@7.28.6_@jest+transform@29.7.0_@jest+types@29.6.3_babel-jest@_d5a6b0105c56738d28f3226733e478bc/node_modules:/Users/pbeenig/Work/远程工作/临行网络科技/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
